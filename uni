def plot():
    node_count = 0
    for y in range(0, 5):
        for x in range(0, 9 - 2*y):
            node_count += 1
            print(f"\t\t\\node at ({x}, {y})" + "[circle, fill, inner sep=1pt]{};")
            print(f"\t\t\\draw ({x -.2}, {y -.2}) " + "node{" + str(node_count) + "};")

plot()